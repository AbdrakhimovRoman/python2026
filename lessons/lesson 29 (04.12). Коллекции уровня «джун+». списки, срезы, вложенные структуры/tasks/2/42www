
versions = ['v0.1', 'v0.2', 'v0.3', 'v0.4', 'v0.5', 'v0.6', 'v0.7', 'v0.8', 'v0.9']
presentation = versions[:3]
print("Презентация:", presentation)
latest_versions = versions[-4:]
print("Последние версии:", latest_versions)
middle_versions = [versions[2], versions[7]]
print("Средина спринта:", middle_versions)
copy_without_edges = versions[1:-1]
print("Без краев:", copy_without_edges)










