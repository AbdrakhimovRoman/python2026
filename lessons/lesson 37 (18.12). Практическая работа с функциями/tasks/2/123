def delivery_price(city, weight_kg, urgent):
    if urgent:
        return 500 + 30 * weight_kg
    else:
        return 300 + 20 * weight_kg
        
print(delivery_price("Москва", 10, True ))
print(delivery_price("Санкт-Петербург", 5, False )) 
print(delivery_price("Москва", 10, True))
print(delivery_price("Санкт-Петербург", 5, False))

print(delivery_price("Москва", "10", True))
